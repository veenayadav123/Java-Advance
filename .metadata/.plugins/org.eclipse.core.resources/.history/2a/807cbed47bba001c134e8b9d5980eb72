package in.co.rays.bean;

public class Loginbean {

	private int id;
	private String user;
	private String pwd;

	public int getId() {
		return id;
	}

	public void setId() {
		this.id = id;
	}

	public String getUser() {
		return user;
	}

	public void setUser(String user) {
		this.user = user;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
}
